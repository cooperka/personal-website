{
  "name": "personal-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-static start",
    "checkAll": "yarn run lint --fix && yarn run test --updateSnapshot --watchAll=0",
    "format": "prettier --write 'src/**/*.js' --write '*.js' --write '*.md'",
    "lint": "eslint --ext .js .",
    "test": "react-scripts test",
    "clean": "rm -rf build",
    "build": "react-static build",
    "analyze": "yarn run build --analyze",
    "deploy": "yarn run build && firebase deploy",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@material-ui/core": "4.11.2",
    "fontsource-roboto": "3.1.5",
    "prop-types": "15.7.2",
    "ramda": "0.27.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hot-loader": "4.13.0",
    "react-static": "7.4.2",
    "react-static-plugin-reach-router": "7.4.2",
    "react-static-plugin-sitemap": "7.4.2",
    "react-static-plugin-source-filesystem": "7.4.2"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.16.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-cooperka": "1.0.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "firebase-tools": "9.1.0",
    "husky": "4.3.6",
    "lsr-tooling": "1.1.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "react-scripts": "4.0.1",
    "react-test-renderer": "17.0.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
