{
  "name": "personal-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "clean": "rm -rf build",
    "build": "yarn run clean && react-scripts build",
    "build:snapshot": "yarn run build && react-snapshot",
    "build:css": "node-sass src/vendor/ -o src/vendor/ && sed -i 's/\\.material \\./.material./' src/vendor/materialize/sass/materialize.css",
    "deploy": "yarn run build:snapshot && firebase deploy",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint . --ext .js,.jsx",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-snapshot": "1.3.0"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-cooperka": "0.2.10",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "firebase-tools": "5.1.1",
    "flow-bin": "0.83.0",
    "react-scripts": "2.0.5",
    "react-test-renderer": "16.5.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
